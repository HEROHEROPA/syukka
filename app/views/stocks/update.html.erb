<div class = table>
<table>
  <tr>
  <th>ID</th>
  <th>製品名</th>
  <th>在庫量</th>
  <th>注文数</th>
  <th>入庫数</th>
  </tr>
<% @stocks.each do |stock| %>
   <tr>
  <th><%= stock.id %></th>
  <th><%= stock.item_number %></th>
  <th><%= stock.stock_amount %></th> 
  </tr>
<% end %>
</table>
</div>



<div class="item-number">
