    <%# modelオプションはテーブル側に保存するカラムが無いとインスタンスを指定できない。 %>
   <%# <%=render 'shared/error_messages', model: f.object %> 
      <div class="sell-btn-contents"> 
      <%= f.submit "出荷指示" ,class:"sell-btn" %> 
     </div>
  <% end %>  
 </div>      

